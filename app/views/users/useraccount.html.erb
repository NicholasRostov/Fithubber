<div class="card" id="profile-main">
                        <div class="pm-overview c-overflow">
                            <div class="pmo-pic">
                                <div class="p-relative">
                                    <a href="">
                                        <img class="img-responsive" src="img/profile-pics/profile-pic-2.jpg" alt="">
                                    </a>


                                    <a href="" class="pmop-edit">
                                        <i class="zmdi zmdi-camera"></i> <span class="hidden-xs">Update Profile Picture</span>
                                    </a>
                                </div>


                                <div class="pmo-stat">
                                    <h2 class="m-0 c-white"><%= @user.full_name %></h2>
                                    <%= @user.profession %>, <%= @user.organization %>
                                </div>
                            </div>

                            <div class="pmo-block pmo-contact hidden-xs">
                                <h2>Contact</h2>

                                <ul>
                                    <li><i class="zmdi zmdi-phone"></i><%= @user.phone_number %></li>
                                    <li><i class="zmdi zmdi-email"></i> <%= @user.email %></li>
                                    <li><i class="zmdi zmdi-pin-account"></i> <%= @user.username %></li>
                                    <li><i class="zmdi zmdi-assignment"></i> <%= @user.dob %></li>
                                    <li>
                                        <i class="zmdi zmdi-pin"></i>
                                        <address class="m-b-0 ng-binding">
                                            <%= @user.address_1 %>,
                                            <%= @user.address_2 %><br>
                                            <%= @user.city %>,
                                            <%= @user.state %>
                                        </address>
                                    </li>
                                </ul>
                            </div>

                            <div class="pmo-block pmo-items hidden-xs">
                                <h2>Connections</h2>

                                <div class="pmob-body">
                                    <div class="row">
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/1.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/2.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/3.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/4.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/5.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/6.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/7.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/8.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/1.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/2.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/3.jpg" alt="">
                                        </a>
                                        <a href="" class="col-xs-2">
                                            <img class="img-circle" src="img/profile-pics/4.jpg" alt="">
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
<p> Name: <%= @user.full_name %></p>
<p> Email: <%= @user.email %></p>
<p> Username: <%= @user.username %></p>
<p> Date of Birth: <%= @user.dob %></p>
<p> Profession: <%= @user.profession %></p>
<p> Organization: <%= @user.organization %></p>

<div><%= link_to "Edit Profile", "/users/#{current_user.id}/edit", method: :get %></div>

<div><%= link_to "Log in your data", "/data/new", method: :get %></div>

<div><% link_to "Edit your data", "/data/1/edit", method: :get %></div>

<a><div><%= link_to "Link Fitbit Account", fit_user_fitbit_oauth2_omniauth_authorize_path %></a></div>


<% if @today_datas %>
<% @today_datas.each do |data| %>
<p>Date: <%= data.date %></p>
<p>Steps: <%= data.steps %></p>
<p>Heart Rate: <%= data.heart_rate %></p>
<p>Distance: <%= data.distance %></p>
<p>Calories: <%= data.calories %></p>
<p>Sleep: <%= data.sleep %></p>
<p>Special: <%= data.special %></p>
 <% end %>
 <% end %>

<%= @user_activity %>
<div><%= @goals %></div>

